<script>
	import gsap from "gsap";
	import ScrollTrigger from "gsap/dist/ScrollTrigger";

	gsap.registerPlugin(ScrollTrigger);

	const tl = gsap.timeline();
	tl.add(gsap.set("#logo", { color: "#FF3333" }));
	tl.add(gsap.set("#red", {y: -500}))
	tl.add(gsap.set("#blue", {y: -500}))
	tl.add(gsap.set("#green", {y: -500}))
	tl.add(gsap.set("#yellow", {y: -500}))
	tl.add(gsap.set("#purple", {y: -500}))
	tl.add(gsap.to("#loading", { opacity: 0, delay: 2 }));
	tl.add(
		gsap.fromTo(
			"#logo",
			{ color: "#FF3333" },
			{ color: "#fff", duration: 1, delay: 0.8 },
		),
	);
	tl.add(
		gsap.fromTo(
			"#fv",
			{ background: "#006400", width: "100%" },
			{ width: 0, background: "#006400", duration: 1, delay: 0.8 },
		),
	);

	tl.add(
		gsap.fromTo(
			"#logo",
			{ scale: 1 },
			{
				scale: 3.0,
				scrollTrigger: {
					trigger: "#space",
					scrub: true,
					start: "top bottom",
					end: "+=1000px",
					pin: "main",
				},
			},
		),
	);
	tl.add(
		gsap.fromTo(
			"#cover",
			{ height: 0 },
			{
				height: "100%",
				scrollTrigger: {
					trigger: "#space",
					scrub: true,
					start: "top+=1000 bottom",
					end: "+=1000px",
					pin: "main",
				},
			},
		),
	);
	tl.add(
		gsap.fromTo(
			"#logo",
			{ color: "#fff" },
			{
				color: "#FAC61E",
				scrollTrigger: {
					trigger: "#space",
					scrub: true,
					start: "top+=1000 bottom",
					end: "+=1000px",
				},
				onComplete: ()=>{
					gsap.fromTo("#red",{y:-500}, {y:0,duration: 0.4, yoyo:true })
					gsap.fromTo("#blue",{y:-500}, {y:0,duration: 0.4,  yoyo:true, delay: 0.2})
					gsap.fromTo("#green",{y:-500}, {y:0, duration: 0.4, yoyo:true,delay: 0.3})
					gsap.fromTo("#purple",{y:-500}, {y:0,duration: 0.4,  yoyo:true, delay: 0.1})
					gsap.fromTo("#yellow",{y:-500}, {y:0,duration: 0.4,  yoyo:true, delay: 0.4})
				},
			},
		),
	);
	tl.add(
		gsap.fromTo(
			"#logo",
			{},
			{
				scrollTrigger: {
					trigger: "#space",
					start: "top+=2000 bottom",
					end: "bottom top",
					scrub: true,
					pin: "main",
				},
			},
		),
	);
</script>

